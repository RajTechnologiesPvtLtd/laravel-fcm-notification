{
  "name": "rajtechnologies/laravel-fcm-notification",
  "description": "Google Firebase Cloud Messaging (FCM) Push Notification Channel for Laravel",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "laravel",
    "fcm",
    "firebase",
    "notifications",
    "google push notification",
    "google cloud push notification"
  ],
  "homepage": "https://github.com/RajTechnologiesPvtLtd/laravel-fcm-notification",
  "version": "1.0.0-dev",
  "authors": [
    {
      "name": "Bhargav Raviya",
      "email": "rajtechnologies@gmail.com",
      "homepage": "http://rajtechnologies.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "rajtechnologies@gmail.com"
  },
    
  "require": {
    "php": ">=5.6.4",
    "guzzlehttp/guzzle": "^6.2|^7.0",
    "illuminate/notifications": "~5.3|^6.0|^7.0|^8.0|^9.0",
    "illuminate/queue": "~5.3|^6.0|^7.0|^8.0|^9.0",
    "illuminate/support": "~5.3|^6.0|^7.0|^8.0|^9.0",
    "illuminate/config": "~5.3|^6.0|^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "RajTechnologies\\FCM\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RajTechnologies\\FCM\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RajTechnologies\\FCM\\FcmNotificationServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
